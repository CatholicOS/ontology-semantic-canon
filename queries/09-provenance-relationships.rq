# Find provenance and derivation relationships
# Tracks document origins, translations, versions, and sources
# Note: Simplified query structure to avoid rdflib 7.x / Python 3.12 compatibility issues

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

SELECT DISTINCT ?property ?label ?definition
WHERE {
  ?property a owl:ObjectProperty .
  ?property rdfs:label ?label .
  OPTIONAL { ?property skos:definition ?definition }
  FILTER(REGEX(STR(?label), "derived|source|version|translated|provenance|edition", "i"))
}
ORDER BY ?label
